<Synthesis>
    <Hardware>
        <Reactor
            id="reactor2" />
    </Hardware>

    <Reagents>
        <Reagent 
            id="sodium_hydroxide" />
        <Reagent
            id="potassium_hydroxide" />
        <Reagent
            id="water" />
        <Reagent
            id="ammonia" />

    </Reagents>

    <Procedure>
         <MakeSolution
            solute="potassium_hydroxide sodium_hydroxide"
            solvent="water"
            solvent_volume="15mL"
            solute_mass="1g"
            vessel="reactor1" />

        <Add
            reagent="ammonia"
            volume="15mL"
            vessel="reactor2" />

    </Procedure>

</Synthesis>