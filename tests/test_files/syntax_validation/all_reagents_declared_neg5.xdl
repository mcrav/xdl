<Synthesis>
    <Hardware>
        <Reactor
            id="reactor1" />
    </Hardware>

    <Reagents>

        <Reagent
            id="potassium_hydroxide" />
        <Reagent
            id="water" />
        <Reagent
            id="ammonia" />
        <Reagent
            id="ethanol" />
    </Reagents>

    <Procedure>
         <MakeSolution
            solute="potassium_hydroxide sodium_hydroxide"
            solvent="water"
            solvent_volume="15mL"
            solute_mass="1g"
            vessel="reactor1" />

        <Add
            reagent="ammonia"
            volume="15mL"
            vessel="reactor1" />

        <CleanVessel
            solvent="ethanol"
            vessel="reactor1" />
    </Procedure>

</Synthesis>