<!-- Tests unclosed steps, Add, StartStir, StartHeat, Wait and defaults -->
<Synthesis>
    <Hardware>
        <Reactor
            id="reactor1" />
    </Hardware>
    <Reagents>
        <Reagent
            id="ammonia" />
    </Reagents>
    <Procedure>
        <Add
            reagent="ammonia"
            volume="10mL"
            vessel="reactor1" />
        <StartStir
            vessel="reactor1" />
        <StartHeat
            vessel="reactor1"
            temp="50" />
        <Wait
            time="5" />
        
    </Procedure>
</Synthesis>
