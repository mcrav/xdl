
<Synthesis autoClean="false">
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
  </Hardware>

  <Reagents>
    <Reagent
      id="NaCl" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <Add
      reagent="water"
      vessel="rotavap"
      mass="500 mL" />
    <Rotavap
      rotavap_name="rotavap"
      temp="40"
      pressure="50" />

  </Procedure>

</Synthesis>
