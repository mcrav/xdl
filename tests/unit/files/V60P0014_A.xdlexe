          rotavap_name="rotavap"
          temp="25°C" />
        <CRotavapStartHeater
          rotavap_name="rotavap" />
      </RotavapHeatToTemp>
      <CRotavapAutoEvaporation
        rotavap_name="rotavap"
        sensitivity="2"
        vacuum_limit="1"
        time_limit="30 mins"
        vent_after="True" />
