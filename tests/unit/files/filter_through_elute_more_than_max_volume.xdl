<Synthesis auto_clean="false">
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
  </Hardware>

  <Reagents>
    <Reagent
      id="NaCl" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <FilterThrough
      from_vessel="reactor"
      to_vessel="rotavap"
      through="celite"
      eluting_solvent="ether"
      eluting_volume="1 L"
      eluting_repeats="1" />
    <FilterThrough
      from_vessel="reactor"
      to_vessel="rotavap"
      through="celite"
      eluting_solvent="ether"
      eluting_volume="1 L"
      eluting_repeats="2" />
    <FilterThrough
      from_vessel="reactor"
      to_vessel="rotavap"
      through="celite"
      eluting_solvent="ether"
      eluting_volume="25 mL"
      eluting_repeats="3" />
    <FilterThrough
      from_vessel="reactor"
      to_vessel="rotavap"
      through="celite"
      eluting_solvent="ether"
      eluting_volume="50 mL"
      eluting_repeats="3" />
  </Procedure>

</Synthesis>
