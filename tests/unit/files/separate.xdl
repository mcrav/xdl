<Synthesis auto_clean="false">
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
    <Component
      id="filter"
      type="filter"
    />
    <Component
      id="reactor"
      type="reactor"
    />
  </Hardware>

  <Reagents>
    <Reagent id="water" />
    <Reagent id="ether" />
  </Reagents>

  <Procedure>
    <Separate
      purpose="wash"
      from_vessel="filter"
      separation_vessel="separator"
      to_vessel="rotavap"
      to_port="evaporate"
      solvent="ether"
      product_bottom="True"
      solvent_volume="50 mL"
      n_separations="1" />
    <Separate
      purpose="wash"
      from_vessel="filter"
      separation_vessel="separator"
      to_vessel="rotavap"
      to_port="evaporate"
      solvent="ether"
      product_bottom="False"
      solvent_volume="50 mL"
      n_separations="1" />
    <Separate
      purpose="extract"
      from_vessel="filter"
      separation_vessel="separator"
      to_vessel="rotavap"
      to_port="evaporate"
      solvent="ether"
      product_bottom="True"
      solvent_volume="50 mL"
      n_separations="1" />
    <Separate
      purpose="extract"
      from_vessel="filter"
      separation_vessel="separator"
      to_vessel="rotavap"
      to_port="evaporate"
      solvent="ether"
      product_bottom="False"
      solvent_volume="50 mL"
      n_separations="1" />
    <Separate
      purpose="wash"
      from_vessel="filter"
      separation_vessel="separator"
      to_vessel="rotavap"
      to_port="evaporate"
      solvent="ether"
      product_bottom="True"
      solvent_volume="50 mL"
      n_separations="2" />
    <Separate
      purpose="wash"
      from_vessel="filter"
      separation_vessel="separator"
      to_vessel="rotavap"
      to_port="evaporate"
      solvent="ether"
      product_bottom="False"
      solvent_volume="50 mL"
      n_separations="2" />
    <Separate
      purpose="extract"
      from_vessel="filter"
      separation_vessel="separator"
      to_vessel="rotavap"
      to_port="evaporate"
      solvent="ether"
      product_bottom="True"
      solvent_volume="50 mL"
      n_separations="2" />
    <Separate
      purpose="extract"
      from_vessel="filter"
      separation_vessel="separator"
      to_vessel="rotavap"
      to_port="evaporate"
      solvent="ether"
      product_bottom="False"
      solvent_volume="50 mL"
      n_separations="2" />
  </Procedure>

</Synthesis>
