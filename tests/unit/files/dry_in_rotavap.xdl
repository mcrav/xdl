<Synthesis filter_dead_volume_method="solvent" auto_clean="false">
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
  </Hardware>

  <Reagents>
    <Reagent id="NaCl" />
    <Reagent id="ether" />
  </Reagents>

  <Procedure>
    <Add
      vessel="rotavap"
      reagent="NaCl"
      mass="5g"
    />
    <Dry
      vessel="rotavap"
      vacuum_pressure="600"
      time="2 hrs"
    />

  </Procedure>

</Synthesis>
