<Synthesis
  graph_sha256="b7d05d8b742a8f91f975ae32088e302f82dff2f1760e47abace0d42de78df469"
>
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap" />
  </Hardware>

  <Reagents>
    <Reagent
      id="NaCl" />
    <Reagent
      id="water" />
  </Reagents>

  <Procedure>
    <CConnect
      from_vessel="flask_nitrogen"
      to_vessel="filter"
      from_port="None"
      to_port="bottom"
      unique="True" />
    <Add
      reagent="water"
      vessel="rotavap"
      volume="None"
      mass="500 g"
      port="None"
      through="None"
      move_speed="40"
      aspiration_speed="10"
      dispense_speed="40"
      time="None"
      stir="False"
      stir_speed="250 RPM"
      anticlogging="False"
      anticlogging_solvent="None"
      anticlogging_solvent_volume="2 mL"
      anticlogging_reagent_volume="10 mL"
      anticlogging_solvent_vessel="None"
      reagent_vessel="flask_water"
      waste_vessel="waste_rotavap"
      flush_tube_vessel="flask_nitrogen"
      vessel_type="rotavap">
      <Confirm
        msg="Is water (500.0 g) in rotavap?" />
    </Add>
    <Evaporate
      rotavap_name="rotavap"
      temp="40°C"
      pressure="50 mbar"
      time="2 hrs"
      rotation_speed="150"
      mode="manual"
      waste_vessel="waste_rotavap"
      collection_flask_volume="50 mL">
      <RotavapStartRotation
        rotavap_name="rotavap"
        rotation_speed="150">
        <CRotavapSetRotationSpeed
          rotavap_name="rotavap"
          rotation_speed="150" />
        <CRotavapStartRotation
          rotavap_name="rotavap" />
      </RotavapStartRotation>
      <CRotavapLiftDown
        rotavap_name="rotavap" />
      <RotavapHeatToTemp
        rotavap_name="rotavap"
        temp="40°C">
        <CRotavapSetTemp
          rotavap_name="rotavap"
          temp="40°C" />
        <CRotavapStartHeater
          rotavap_name="rotavap" />
      </RotavapHeatToTemp>
      <RotavapStartVacuum
        rotavap_name="rotavap"
        pressure="50 mbar">
        <CSetVacuumSetPoint
          vessel="rotavap"
          vacuum_pressure="50 mbar" />
        <CStartVacuum
          vessel="rotavap" />
      </RotavapStartVacuum>
      <Wait
        time="2 hrs"
        wait_recording_speed="2000"
        after_recording_speed="14">
        <CSetRecordingSpeed
          recording_speed="2000" />
        <CWait
          time="2 hrs" />
        <CSetRecordingSpeed
          recording_speed="14" />
      </Wait>
      <RotavapStopEverything
        rotavap_name="rotavap">
        <CStopVacuum
          vessel="rotavap" />
        <CRotavapLiftUp
          rotavap_name="rotavap" />
        <CVentVacuum
          vessel="rotavap" />
        <CRotavapStopHeater
          rotavap_name="rotavap" />
        <CRotavapStopRotation
          rotavap_name="rotavap" />
      </RotavapStopEverything>
      <Transfer
        from_vessel="rotavap"
        to_vessel="waste_rotavap"
        volume="50 mL"
        from_port="collect"
        to_port="0"
        through="None"
        time="None"
        aspiration_speed="10"
        move_speed="40"
        dispense_speed="40">
        <CMove
          from_vessel="rotavap"
          to_vessel="waste_rotavap"
          volume="50 mL"
          move_speed="40"
          aspiration_speed="10"
          dispense_speed="40"
          from_port="collect"
          to_port="0"
          unique="False"
          through="None" />
      </Transfer>
    </Evaporate>
  </Procedure>

</Synthesis>
