

<Synthesis auto_clean="false">
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
  </Hardware>

  <Reagents>
    <Reagent id="ether" />
  </Reagents>

  <Procedure>
    <Add
      vessel="filter"
      reagent="ether"
      volume="15 mL"
      port="top"
    />
    <Filter filter_vessel="filter" />
    <WashFilterCake
      filter_vessel="filter"
      solvent="ether"
    />
    <Dry
      vessel="filter"
      vacuum_pressure="600"
      time="2 hrs"
    />

  </Procedure>

</Synthesis>