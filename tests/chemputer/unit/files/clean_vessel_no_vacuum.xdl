<Synthesis auto_clean="false">
  <Hardware>
    <Component
      id="rotavap"
      type="rotavap"
    />
    <Component
      id="reactor"
      type="reactor"
    />
  </Hardware>

  <Reagents>
    <Reagent
      id="NaCl" />
    <Reagent
      id="toluene" />
  </Reagents>

  <Procedure>
    <CleanVessel
      solvent="toluene"
      vessel="separator" />
  </Procedure>

</Synthesis>
